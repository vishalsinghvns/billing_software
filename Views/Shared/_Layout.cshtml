    <script>
        isNumberOrDecimal();
        function isNumberOrDecimal() {
            $(".number_decimal").on("keypress", function (evt) {
                var charCode = (evt.which) ? evt.which : evt.keyCode;
                if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57)) {
                    swal({
                        title: "Message:",
                        text: "Only Decimal Or Number Values",
                        type: "error"
                    });
                    return false;
                }
                return true;
            });
        }
    </script>

